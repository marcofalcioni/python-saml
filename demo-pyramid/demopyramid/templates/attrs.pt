<div metal:use-macro="load: base.pt">
	<div metal:fill-slot="content">
		<div tal:switch="paint_logout">
		    <div tal:case="True">
		        <div tal:switch="python:len(attributes) > 0">
		            <div tal:case="True">
		                <table class="table table-striped">
		                    <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Values</th>
                                </tr>
		                    </thead>
		                    <tbody>
								<tr tal:repeat="attr attributes">
								<td tal:content="attr[0]">Attribute Name</td>
									<td>
										<ul>
											<li tal:repeat="detail attr[1]">
												<span tal:replace="detail" />
											</li>
										</ul>
									</td>
								</tr>
		                    </tbody>
		                </table>
		            </div>
		            <div tal:case="False">
		                <div class="alert alert-danger" role="alert">You don't have any attributes</div>
		            </div>
		        </div>
                <a href="/?slo" class="btn btn-danger">Logout</a>
		    </div>
		    <div tal:case="False">
		        <a href="?sso" class="btn btn-primary">Login</a>
		        <a href="?sso2" class="btn btn-info">Login and access to attrs page</a>
		    </div>
		</div>
	</div>
</div>